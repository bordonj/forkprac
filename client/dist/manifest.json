{
  "manifest_version": 2,
  "name": "Tailored Search",
  "author": "sparksparker",
  "version": "1.0.0",
  "description": "Tailor your Google searches every time",
  "icons": {
    "16": "./16.png",
    "32": "./32.png",
    "48": "./48.png",
    "128": "./128.png"
  },
  "browser_action": {
    "default_popup": "./index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}